<?xml version="1.0" encoding="utf-8"?>
<configuration>
	
  <!-- Protecting the admin UI -->
  <location path="Views/Admin">
    <system.web>
      <httpRuntime executionTimeout="1000" />
      <authorization>
        <allow roles="WebAdmins, CmsAdmins, Administrators" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>

</configuration>